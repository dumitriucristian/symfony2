{# app/Resources/views/Default/base.html.twig #}
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <title>{% block title %}Test Application{% endblock %}</title>
    </head>
    <body>
        <div id="header">
         {% block header %}
            <h1>This is the unique header - after the first commit to the second</h1>
        {% endblock %}
        </div>
        <div id="sidebar">
            {% block sidebar %}
                <ul>
                      <li><a href="/">Home</a></li>
                      <li><a href="/blog">Blog</a></li>
                </ul>
            {% endblock %}
        </div>

        <div id="content">
            {% block content %}
                <p>Here is the general content</p> 
            {% endblock %}
        </div>
        <div id="footer">
            {% block footer %}
                <p>Here is general footer</p>
            {% endblock %}
        </div>
    </body>
</html>